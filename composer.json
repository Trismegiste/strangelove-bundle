{
    "name": "trismegiste/toolbox",
    "description": "Library of useful tools",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Trismegiste",
            "email": "flo@trismegiste.net"
        }
    ],
    "require": {
        "php": ">7.3",
        "ext-mongodb": ">1.7"
    },
    "autoload": {
        "psr-4": {
            "Trismegiste\\Toolbox\\": "src/"
        },
        "files": ["src/util.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Fixtures\\": "tests/fixtures/",
            "Tests\\Toolbox\\": "tests/"
        }
    }
}
